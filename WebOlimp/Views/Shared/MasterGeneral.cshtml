@using WebOlimp.Entities;
@{
    Layout = null;
    var sesionactual = (ResponseTokenModel)Session["sesion"];
}

<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Expires" content="0">
    <meta http-equiv="Last-Modified" content="0">
    <meta http-equiv="Cache-Control" content="no-cache, no-store, mustrevalidate">
    <meta http-equiv="Pragma" content="no-cache">
    <title>Mantenedores Prueba Tecnica </title>
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,800,800italic">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Oswald:400,300,700">
    <style type="text/css">
        @@font-face {
          font-family: 'FontAwesome';
          src: url('@Url.Content("~/Content/fontawesome/fonts/fontawesome-webfont.eot?v=4.6.3")');
          src: url('@Url.Content("~/Content/fontawesome/fonts/fontawesome-webfont.eot?#iefix&v=4.6.3")') format('embedded-opentype'), url('@Url.Content("~/Content/fontawesome/fonts/fontawesome-webfont.woff2?v=4.6.3")') format('woff2'), url('@Url.Content("~/Content/fontawesome/fonts/fontawesome-webfont.woff?v=4.6.3")') format('woff'), url('@Url.Content("~/Content/fontawesome/fonts/fontawesome-webfont.ttf?v=4.6.3")') format('truetype'), url('@Url.Content("~/Content/fontawesome/fonts/fontawesome-webfont.svg?v=4.6.3#fontawesomeregular")') format('svg');
          font-weight: normal;
          font-style: normal;
        }
    </style>
    @Styles.Render("~/Content/css")
    @RenderSection("stylesowner", required: false)
    @Styles.Render("~/Content/styles/owner")
</head>
<body>

    <div class="page-wrapper chiller-theme toggled">
        <a id="show-sidebar" class="btn btn-sm btn-dark" href="#">
            <i class="fa fa-bars"></i>
        </a>
        <nav id="sidebar" class="sidebar-wrapper">
            <div class="sidebar-content">
                <div class="sidebar-brand">
                    <a href="#">Mantenedor Prueba técnica</a>
                    <div id="close-sidebar">
                        <i class="fa fa-times"></i>
                    </div>
                </div>

                <div class="sidebar-menu">
                    <ul>
                        <li class="header-menu">
                            <span>Módulos</span>
                        <li>
                                <ul class="xp-vertical-submenu">
                                    <li>
                                        <a href="@Url.Content($"~/Sede/ListaSede")">
                                            Sede
                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Content($"~/ComplejoPolideportivo/ListaComplejoPolideportivo")">
                                            Complejo Polideportivo
                                        </a>
                                    </li>
                                </ul>
                        </li>
                    </ul>
                </div>
                <!-- sidebar-menu  -->
            </div>
            <!-- sidebar-content  -->
            <div class="sidebar-footer">
                <a href="@Url.Action("Login", "Account")">
                    <i class="fa fa-power-off"></i>
                </a>
            </div>
        </nav>

        <main class="page-content">
            <div class="container-fluid">
                @RenderBody()
            </div> <!-- .content -->
        </main> <!-- /.content-page -->
    </div> <!-- /#wrapper -->

    <script>
         var rutaPrincipal = "@Url.Content("~")";
    </script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/General")
    @Scripts.Render("~/bundles/bootstrap/end")
    @RenderSection("scripts", required: false)
</body>
</html>

